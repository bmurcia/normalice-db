        "optional": ["total", "impuesto", "subtotal", "fecha", "producto", "cantidad", "precio"]
      },
      "entities": ["CLIENTES", "PRODUCTOS", "FACTURAS", "DETALLE_FACTURAS"],
      "tableStructure": {
        "CLIENTES": {
          "purpose": "Información de clientes",
