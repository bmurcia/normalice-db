            { "name": "id_cliente", "type": "INTEGER", "isPrimaryKey": true, "isForeignKey": false },
            { "name": "nombre", "type": "VARCHAR(255)", "isPrimaryKey": false, "isForeignKey": false },
            { "name": "email", "type": "VARCHAR(255)", "isPrimaryKey": false, "isForeignKey": false }
          ],
          "relationships": []
        },
        "PRODUCTOS": {
          "purpose": "Catálogo de productos",
          "columns": [
            { "name": "id_producto", "type": "INTEGER", "isPrimaryKey": true, "isForeignKey": false },
            { "name": "nombre", "type": "VARCHAR(255)", "isPrimaryKey": false, "isForeignKey": false },
            { "name": "precio", "type": "DECIMAL(10,2)", "isPrimaryKey": false, "isForeignKey": false }
          ],
          "relationships": []
        },
        "FACTURAS": {
          "purpose": "Cabecera de facturas",
